
public class ParamsArray {

	public static void main(String[] args) {
		
		printNumbers(1);
		
//		printWords("Lovely");
//		
//		printWords("Apple", "Orrange", "Bannana");
//		
//		printWords("Lada", "Opel", "BMV", "WV");

	}
	
	public static void printNumbers(int... words) {
	    for (int word : words) {
	        System.out.print(word + " ");
	    }
	    
	    System.out.println();
	}

}
