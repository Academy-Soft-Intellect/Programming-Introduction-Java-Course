public class VariableScope {

	public static void main(String[] args) {
		
		int d = 12;
		int[] arr = {1,2,3};
		int d111 = print(arr, d);
		

	}
	
	public static int print(int[] args, int d) {
		
		d = 12;
		
		for(int item : args){
			int p = 5;
			System.out.println(item);
		}
		//System.out.println(p);
		
		System.out.println(d);
		return d;
		
	}
}
