public class VariableScope {

	public static void main(String[] args) {

		int[] arr = {1,2,3};
		print(arr);
		System.out.println(d);

	}
	
	public static void print(int[] args) {
		
		int d = 12;
		
		for(int item : args){
			int p = 5;
			System.out.println(item);
		}
		//System.out.println(p);
		
		System.out.println(d);
		
	}
}
