public class VariableScope {

	public static void main(String[] args) {

		int[] arr = {1,2,3};
		int d = print(arr);
		

	}
	
	public static int print(int[] args) {
		
		int d = 12;
		
		for(int item : args){
			int p = 5;
			System.out.println(item);
		}
		//System.out.println(p);
		
		System.out.println(d);
		return d;
		
	}
}
